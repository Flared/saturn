#!/usr/bin/env bash

set -eo pipefail
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"
ROOT_DIR="$( cd ${DIR}/.. && pwd )"

export SATURN_ENV="${SATURN_ENV:-development}"
export SATURN_SETTINGS="example.settings.config"
export PYTHONPATH="${PYTHONPATH}:$DIR/src"
exec bash ${ROOT_DIR}/bin/worker
